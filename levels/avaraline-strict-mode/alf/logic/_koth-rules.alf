<Teleporter start="mGameEnds" y="-10" activeRange="10000" shape="0" win="0" cx="0" cz="0" />

<Timer in.0="@start" in.1="pPulse" wait="15" out.0="pPulse" />
<Timer in="pPulse" wait="1" out="pPulseEnd" />
