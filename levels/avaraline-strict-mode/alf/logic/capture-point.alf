<And
  in="mBlackEnters"
  out.0="mNeutralOff"
  out.1="mGreenOff"
  out.2="mYellowOff"
  out.3="mRedOff"
  out.4="mPinkOff"
  out.5="mPurpleOff"
  out.6="mBlueOff"
  out.7="mWhiteOff"
