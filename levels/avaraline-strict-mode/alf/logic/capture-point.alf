<!-- @param int pCaptureVizShape the bsp resource ID for the capture point visual indicator -->
<!-- @param number pCaptureVizScale the scale for the capture point visual indicator -->
<!-- @param number pCaptureVizSpeed the spin speed for the capture point visual indicator -->
<!-- @param int pGreenPoint triggering event for adding a point to the green team -->
<!-- @param int pYellowPoint triggering event for adding a point to the yellow team -->
<!-- @param int pRedPoint triggering event for adding a point to the red team -->
<!-- @param int pPinkPoint triggering event for adding a point to the pink team -->
<!-- @param int pPurplePoint triggering event for adding a point to the purple team -->
<!-- @param int pBluePoint triggering event for adding a point to the blue team -->
<!-- @param int pBlackPoint triggering event for adding a point to the black team -->
<!-- @param int pWhitePoint triggering event for adding a point to the white team -->
<!-- @param int pCaptureDoorZOffset offset for the z-coordinate for this capture point's door (handled automatically) -->

<unique vars="mGreenFieldOn mYellowFieldOn mRedFieldOn mPinkFieldOn mPurpleFieldOn mBlueFieldOn mBlackFieldOn mWhiteFieldOn" />
<unique vars="mGreenFieldSuspend mYellowFieldSuspend mRedFieldSuspend mPinkFieldSuspend mPurpleFieldSuspend mBlueFieldSuspend mBlackFieldSuspend mWhiteFieldSuspend" />
<unique vars="mGreenEnters mYellowEnters mRedEnters mPinkEnters mPurpleEnters mBlueEnters mBlackEnters mWhiteEnters" />
<unique vars="mGreenOpen mYellowOpen mRedOpen mPinkOpen mPurpleOpen mBlueOpen mBlackOpen mWhiteOpen" />
<unique vars="mGreenDidOpen mYellowDidOpen mRedDidOpen mPinkDidOpen mPurpleDidOpen mBlueDidOpen mBlackDidOpen mWhiteDidOpen" />
<unique vars="mGreenClose mYellowClose mRedClose mPinkClose mPurpleClose mBlueClose mBlackClose mWhiteClose" />
<unique vars="mNeutralOff mGreenOff mYellowOff mRedOff mPinkOff mPurpleOff mBlueOff mBlackOff mWhiteOff" />

<And
  in="@start"
  out.0="mGreenFieldOn"
  out.1="mYellowFieldOn"
  out.2="mRedFieldOn"
  out.3="mPinkFieldOn"
  out.4="mPurpleFieldOn"
  out.5="mBlueFieldOn"
  out.6="mBlackFieldOn"
  out.7="mWhiteFieldOn"
/>

<Timer in="mGreenFieldSuspend" wait="2" out="mGreenFieldOn" />
<Timer in="mYellowFieldSuspend" wait="2" out="mYellowFieldOn" />
<Timer in="mRedFieldSuspend" wait="2" out="mRedFieldOn" />
<Timer in="mPinkFieldSuspend" wait="2" out="mPinkFieldOn" />
<Timer in="mPurpleFieldSuspend" wait="2" out="mPurpleFieldOn" />
<Timer in="mBlueFieldSuspend" wait="2" out="mBlueFieldOn" />
<Timer in="mBlackFieldSuspend" wait="2" out="mBlackFieldOn" />
<Timer in="mWhiteFieldSuspend" wait="2" out="mWhiteFieldOn" />

<Field deltaY="0" mask="T1" start="mGreenFieldOn" stop="mGreenFieldSuspend" enter="mGreenEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />
<Field deltaY="0" mask="T2" start="mYellowFieldOn" stop="mYellowFieldSuspend" enter="mYellowEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />
<Field deltaY="0" mask="T3" start="mRedFieldOn" stop="mRedFieldSuspend" enter="mRedEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />
<Field deltaY="0" mask="T4" start="mPinkFieldOn" stop="mPinkFieldSuspend" enter="mPinkEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />
<Field deltaY="0" mask="T5" start="mPurpleFieldOn" stop="mPurpleFieldSuspend" enter="mPurpleEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />
<Field deltaY="0" mask="T6" start="mBlueFieldOn" stop="mBlueFieldSuspend" enter="mBlueEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />
<Field deltaY="0" mask="T7" start="mBlackFieldOn" stop="mBlackFieldSuspend" enter="mBlackEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />
<Field deltaY="0" mask="T8" start="mWhiteFieldOn" stop="mWhiteFieldSuspend" enter="mWhiteEnters" x="pCaptureX" y="pCaptureY" z="pCaptureZ" w="pCaptureW" d="pCaptureD" h="pCaptureH" />

<And in="mGreenEnters" out="mGreenOpen" />
<And in="mYellowEnters" out="mYellowOpen" />
<And in="mRedEnters" out="mRedOpen" />
<And in="mPinkEnters" out="mPinkOpen" />
<And in="mPurpleEnters" out="mPurpleOpen" />
<And in="mBlueEnters" out="mBlueOpen" />
<And in="mBlackEnters" out="mBlackOpen" />
<And in="mWhiteEnters" out="mWhiteOpen" />

<And
  in="mGreenEnters"
  out.0="mNeutralOff"
  out.1="mYellowOff"
  out.2="mRedOff"
  out.3="mPinkOff"
  out.4="mPurpleOff"
  out.5="mBlueOff"
  out.6="mBlackOff"
  out.7="mWhiteOff"
/>
<And
  in="mYellowEnters"
  out.0="mNeutralOff"
