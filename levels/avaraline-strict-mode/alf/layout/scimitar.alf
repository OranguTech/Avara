<SkyColor color="#fffff0" color.1="#bde8ff" thickness="3000" verticalRangeMin="1000" verticalRangeMax="6000" />
<GroundColor color="#fff9d9" color.1="#000000" material.specular="$sand.specular" material.shininess="sand.shininess" />

<!-- CENTER -->
<Wall d="15" color="#262522" material.specular="$basalt.specular" material.shininess="basalt.shininess" h="3" w="15" x="0" z="0" />
