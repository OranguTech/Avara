<Ramp angle="270" d="15" deltaY="3" color="#82d9cb" material.specular="$metal.specular" material.shininess="metal.shininess" h="0" w="10" x="-12.5" y="0" z="0" />
<Ramp angle="90" d="15" deltaY="3" color="#82d9cb" material.specular="$metal.specular" material.shininess="metal.shininess" h="0" w="10" x="12.5" y="0" z="0" />

<set
