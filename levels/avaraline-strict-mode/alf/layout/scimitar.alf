<set
  designer="$mSilverfox"
  information="Between the sun, sand, and glint of metal, it was nearly impossible to trust my eyes, but I swear the place was not as I left it."
  ambient.c="#fffff0"
  light.0.i="0.9"
  light.0.a="30"
  light.0.b="10"
  light.0.r="128.0"
  light.0.s="true"
  baseMaterial.specular="#54544f"
  baseMaterial.shininess="8.0"
  sand.specular="#33322b"
  sand.shininess="4.0"
  basalt.specular="#555458"
