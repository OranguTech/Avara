<map>
  <set designer="Head" information="A fast rendering, fast playing level. Now with random incarnators at the boundary, no-spin teleporter, 7 lives." defaultLives="7" missilePower="1.8" missileAcceleration="0.3" startY="8" />
  <set grenDelay="250" numGren="6" />
  <set missDelay="250" numMiss="4" />
  <SkyColor fill="#fffc41" frame="#ff7c00" />
  <GroundColor fill="#d6a800" />
  <Sound y="50" sound="snArcticWind" isAmbient="true" loopCount="-1" volume="80" cx="30" cz="30" angle="225" />

  <Teleporter shape="0" y="200" win="1200" start="@winTeam" activeRange="2000" fill="#007600" frame="#000000" cx="0" cz="-32.708333" angle="225" />
  <Teleporter group="@center" y="-2 * startY" activeRange="500" deadRange="48" destGroup="@recenter" shape="0" fill="#000000" frame="#000000" cx="0" cz="0" angle="0" />
  <Teleporter y="startY" group="@recenter" shape="bspGroundStar" speed="0" fill="#000000" frame="#000000" cx="0" cz="0" angle="0" spin="false" />
  <Hologram fill="#000000" frame="#000000" cx="0" cz="0" angle="0" />

  <Goody shape="bspGroundStar" scale="0.618" yon="500" sound="0" y=".001" speed="1.2" grenades="0" start="@start" out="@start" fill="#000000" frame="#000000" cx="0" cz="0" angle="0" />
  <Goody shape="bspGroundStar" scale="0.618" yon="500" sound="0" y="startY" speed="-1.2" grenades="0" start="@start" out="@start" fill="#000000" frame="#000000" cx="0" cz="0" angle="45" />

  <!-- <unique vars="mShow" />
  <Base in="@start" out="mShow" cx="0" cz="0" />
  <Goody shape="bspGroundStar" scale="0.618" yon="500" sound="0" y=".001" speed="1" grenades="0" start="@start" out="mShow" fill="#c02020" frame="#000000" cx="0" cz="0" angle="45" /> -->
  <!-- <Goody shape="bspGroundStar" scale="0.618" yon="500" sound="0" y="startY-0.001" speed="-1" grenades="0" start="@start" out="mShow" fill="#c02020" frame="#000000" cx="0" cz="0" angle="45" /> -->

  <!-- <Hologram visible="true" y="2" shape="bspLargeDome" scale="20" fill="#000000" frame="#000000" cx="0" cz="0" angle="0"  roll="180" /> -->
  <Dome hasFloor="true" fill="rgba(0, 0, 0, 0.04)" frame="#000000" cx="0" cz="0" y="-0.01" r="50" roll="180" angle="0" quarters="4" />
  <!-- <Dome hasFloor="true" fill="#000000" frame="#000000" cx="0" cz="0" y="-0.02" r="51" roll="180" angle="0" quarters="4" /> -->
  <!-- <Dome hasFloor="true" fill="rgba(214, 168, 0, 1)" frame="#000000" cx="0" cz="0" y="-0.01" r="3" roll="0" angle="0" quarters="4" /> -->


  <!-- Grenades -->
  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="grenDelay" />
  <Goody shape="bspGrenade" y="2" grenades="numGren" start="mShow" out="mTaken" cx="0" cz="12" angle="180" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="grenDelay" />
  <Goody shape="bspGrenade" y="2" grenades="numGren" start="mShow" out="mTaken" cx="10.392305" cz="6" angle="120" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="grenDelay" />
  <Goody shape="bspGrenade" y="2" grenades="numGren" start="mShow" out="mTaken" cx="-10.392305" cz="6" angle="240" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="grenDelay" />
  <Goody shape="bspGrenade" y="2" grenades="numGren" start="mShow" out="mTaken" cx="0" cz="-12" angle="0" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="grenDelay" />
  <Goody shape="bspGrenade" y="2" grenades="numGren" start="mShow" out="mTaken" cx="-10.392305" cz="-6" angle="300" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="grenDelay" />
  <Goody shape="bspGrenade" y="2" grenades="numGren" start="mShow" out="mTaken" cx="10.392305" cz="-6" angle="60" />


  <!-- Missiles -->
  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="missDelay" />
  <Goody shape="bspMissile" y="2" missiles="numMiss" start="mShow" out="mTaken" fill="#0052ff" frame="#000000" cx="42" cz="0" angle="90" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="missDelay" />
  <Goody shape="bspMissile" y="2" missiles="numMiss" start="mShow" out="mTaken" fill="#0052ff" frame="#000000" cx="21" cz="36.373067" angle="150" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="missDelay" />
  <Goody shape="bspMissile" y="2" missiles="numMiss" start="mShow" out="mTaken" fill="#012ce4" frame="#000000" cx="-21" cz="36.373067" angle="210" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="missDelay" />
  <Goody shape="bspMissile" y="2" missiles="numMiss" start="mShow" out="mTaken" fill="#0052ff" frame="#000000" cx="-42" cz="0" angle="270" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="missDelay" />
  <Goody shape="bspMissile" y="2" missiles="numMiss" start="mShow" out="mTaken" fill="#0052ff" frame="#000000" cx="-21" cz="-36.373067" angle="330" />

  <unique vars="mShow mTaken" />
  <And in="@start" out="mShow" />
  <Timer in="mTaken" out="mShow" wait="missDelay" />
  <Goody shape="bspMissile" y="2" missiles="numMiss" start="mShow" out="mTaken" fill="#0052ff" frame="#000000" cx="21" cz="-36.373067" angle="30" />
</map>
